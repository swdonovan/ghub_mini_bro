<div class="mq-panel-wrapper">
  <div class="mq-panel-header">
      <h3>My Recent Activity</h3>
  </div>

  <div class="mq-panel-body">
       <ul class="mq-comment">
                  <div class="mq-comment-body clearfix">
                      <div class="header">
                          <strong class="primary-font"><%= @events.first['type'] %></strong> <small class="pull-right text-muted">
                              <span class="glyphicon glyphicon-time"></span><%= distance_of_time_in_words Time.now, @events.first['created_at'] %></small>
                      </div>
                      <p>
                        <%= @events.first['body'] %>
                      </p>
                  </div>
          </ul>
