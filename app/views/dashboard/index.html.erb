<div class="mq-panel-wrapper">
  <div class="mq-panel-header">
      <h3>Organizations</h3>
  </div>

  <div class="mq-panel-body">
    <% unless @organizations.nil? %>
      <% @organizations.each do |org| %>
       <ul class="mq-comment">
        <div class="mq-comment-body clearfix">
          <div class="header">
            <strong class="primary-font"><%= org['name'] %></strong> <small class="pull-right text-muted">
          </div>
         </div>
       </ul>
      <% end %>
    <% end %>
  </div>
</div>

<div class="mq-panel-wrapper">
  <div class="mq-panel-header">
      <h3>Friends Recent Activity</h3>
  </div>

  <div class="mq-panel-body">
    <% @all_events.each do |event| %>
       <ul class="mq-comment">
                  <div class="mq-comment-body clearfix">
                      <div class="header">
                          <strong class="primary-font"><%= event.first['type'] %></strong> <small class="pull-right text-muted">
                              <span class="glyphicon glyphicon-time"></span><%= distance_of_time_in_words Time.now, event.first['created_at'] %></small>
                      </div>
                      <p>
                        <%= event.first['body'] %>
                      </p>
                  </div>
          </ul>
        <% end %>
  </div>
</div>

<div class="mq-panel-wrapper">
  <div class="mq-panel-header">
      <h3>My Repositories</h3>
  </div>

  <div class="mq-panel-body">
    <% @repos.each do |repo| %>
       <ul class="mq-comment">
          <div class="mq-comment-body clearfix">
              <div class="header">
                  <strong class="primary-font">Name: <%= repo.repo_name %></strong>
                  <strong class="primary-font">Owner: <%= repo.owner_name %></strong>
                  <small class="pull-right text-muted">
                    <span class="glyphicon glyphicon-time"></span>
                   </small>
              </div>
          </div>
        </ul>
    <% end %>
  </div>
</div>

<div class="mq-panel-wrapper">
  <div class="mq-panel-header">
      <h3>Recent Commits</h3>
  </div>

  <div class="mq-panel-body">
    <% @commits.each do |commit| %>
       <ul class="mq-comment">
          <div class="mq-comment-body clearfix">
              <div class="header">
                  <p><strong class="primary-font">Date Created: <%= commit.created_at %></strong></p>
                  <p><strong class="primary-font">Message: <%= commit.message %></strong></p>
                  <small class="pull-right text-muted">
                    <span class="glyphicon glyphicon-time"></span>
                   </small>
              </div>
          </div>
        </ul>
    <% end %>
  </div>
</div>
